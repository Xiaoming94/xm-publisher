<h2>Show Page</h2>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @page.title %>
  </li>

  <li>
    <strong>Slug:</strong>
    <%= @page.slug %>
  </li>

  <li>
    <strong>Body:</strong>
    <%= @page.body %>
  </li>

</ul>

<span><%= link "Edit", to: admin_page_path(@conn, :edit, @page) %></span>
<span><%= link "Back", to: admin_page_path(@conn, :index) %></span>
